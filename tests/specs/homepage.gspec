@objects
    header               #topnav
    TopHeaderLogo        #topnav .logo
    TopHeaderMenu-*      #navigation ul li
    Hero                 .bg-home
    Herosec              .title-heading   
    Heroicon             .title-heading img
    HeroHeading          .title-heading h4
    HeroPara             .title-heading p
    HeroButton           .btn-primary

= Header Section =
    @on desktop
        header:
            centered horizontally inside screen 1px
            height 74px
            inside screen 0px top left right
        TopHeaderLogo:
            width 118px
            height 74px
            inside header 0px top bottom
        TopHeaderMenu-*:
            css font-size is "15px"
            css font-weight is "400"
            css letter-spacing is "normal"
            css line-height is "22.5px"
    @on mobile
        header:
            centered horizontally inside screen 1px
            height 74px
            inside screen 0px top left right
        TopHeaderLogo:
            width 118px
            height 74px
    @on tablet
        header:
            centered horizontally inside screen 1px
            height 74px
            inside screen 0px top left right
        TopHeaderLogo:
            width 118px
            height 74px

= Hero Section =
    @on desktop
        Hero:
            height 995px
            width 1883px
        Heroicon:
            height 36px
            width 41px
        HeroHeading:
            width 1116px
            css line-height is "57.6px"
            css font-size is "48px"
            css font-family is "'Poppins', sans-serif"
            css font-weight is "700"
            inside Herosec 60px top
            inside Herosec 144px bottom
        HeroPara:
            width 600px
            css line-height is "27.2px"
            css font-size is "17px"
            css font-family is "'Roboto', sans-serif"
            css font-weight is "400"
